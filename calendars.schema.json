{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Calendars",
  "type": "object",
  "properties": {
    "date": {
      "type": "string",
      "format": "date"
    },
    "is_holiday": {
      "type": "integer",
      "enum": [
        0,
        1
      ]
    },
    "is_school_break": {
      "type": "integer",
      "enum": [
        0,
        1
      ]
    },
    "event_name": {
      "type": "string"
    },
    "event_type": {
      "type": "string"
    },
    "weather_flag": {
      "type": "string"
    }
  },
  "required": [
    "date"
  ]
}